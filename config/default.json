{
    "rainbowOptions": {
        "rainbow": {
            "host": "<RAINBOW_HOST>"
        },
        "credentials": {
            "login": "<RAINBOW_BOT_LOGIN>",
            "password": "<RAINBOW_BOT_PASSWORD>"
        },
        "application": {
            "appID": "<RAINBOW_APPID>",
            "appSecret": "<RAINBOW_APPSECRET>"
        },
        "logs": {
            "enableConsoleLogs": false,
            "enableFileLogs": true,
            "file": {
                "path": "logs",
                "level": "debug"
            }
        },
        "im": {
            "sendReadReceipt": true,
            "messageMaxLength": 1024,
            "sendMessageToConnectedUser": true,
            "conversationsRetrievedFormat": "small",
            "storeMessages": false,
            "nbMaxConversations": 15,
            "rateLimitPerHour": 1000
        }
    },
    "luis": {
        "ENDPOINT": "<LUIS_ENDPOINT>"
    },
    "openweathermap": {
        "API_Key": "<OWM_API_KEY>",
        "API_URL": "https://api.openweathermap.org/data/2.5/",
        "units": "metric"
    }
}
